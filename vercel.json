{
    "functions": {
        "api/index.go": {
            "includeFiles": "api/static/**"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.go"
        }
    ],
    "buildCommand": "node scripts/build-all.js && node bundler.js",
    "outputDirectory": "api/static"
}