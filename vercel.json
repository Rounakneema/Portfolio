{
    "version": 2,
    "builds": [
        {
            "src": "professional_portfolio/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/blog/(.*)",
            "dest": "/blog/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "buildCommand": "cd professional_portfolio && npm run vercel-build",
    "outputDirectory": "professional_portfolio/dist"
}